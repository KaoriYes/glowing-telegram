{
  "manifest_version": 3,
  "name": "Qt Extension",
  "description": "Select text in MAIN of a website with two clicks",
  "version": "1.3",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
    "matches": ["<all_urls>"],
    "js": ["selectv2.js"]
  }]
}
